    "import math\n",
    "\n",
    "# example of a piecewise function\n",
    "def f(x):\n",
    "    if x <= -2:\n",
    "        y = 2\n",
    "    else:\n",
    "        if x < 0:\n",
    "            y = -x\n",
    "        else: \n",
    "            y = math.floor(x)\n",
    "    return y"
   ]
